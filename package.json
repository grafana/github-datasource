{
  "name": "grafana-github-datasource",
  "version": "1.7.2",
  "private": true,
  "description": "loads data from github issues/Pr's to Grafana",
  "repository": "github:grafana/github-datasource",
  "license": "Apache-2.0",
  "author": "Grafana Labs <team@grafana.com> (https://grafana.com)",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "e2e": "playwright test",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn run lint --fix",
    "server": "docker-compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,go,md,mdx,yml,yaml,json,scss,css}\"",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/css": "11.10.6",
    "@grafana/data": "10.4.2",
    "@grafana/experimental": "^1.7.3",
    "@grafana/runtime": "10.4.2",
    "@grafana/schema": "10.4.2",
    "@grafana/ui": "10.4.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tslib": "2.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@grafana/eslint-config": "^7.0.0",
    "@grafana/plugin-e2e": "^1.2.0",
    "@grafana/plugin-meta-extractor": "^0.0.2",
    "@grafana/tsconfig": "^1.2.0-rc1",
    "@playwright/test": "^1.44.0",
    "@swc/core": "^1.3.90",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/glob": "^8.0.0",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.194",
    "@types/node": "^20.8.7",
    "@types/react-router-dom": "^5.2.0",
    "@types/testing-library__jest-dom": "5.14.8",
    "copy-webpack-plugin": "^11.0.0",
    "cspell": "6.13.3",
    "css-loader": "^6.7.3",
    "eslint": "8.52.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "glob": "^10.2.7",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^3.0.3",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.63.2",
    "sass-loader": "13.3.1",
    "semver": "^7.6.2",
    "style-loader": "3.3.3",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "4.8.4",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "resolutions": {
    "jackspeak": "2.1.1"
  }
}
