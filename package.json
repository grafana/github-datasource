{
  "name": "grafana-github-datasource",
  "version": "1.1.0",
  "description": "loads data from github issues/Pr's to Grafana",
  "private": true,
  "scripts": {
    "build": "grafana-toolkit plugin:build",
    "test": "grafana-toolkit plugin:test",
    "e2e": "grafana-e2e run",
    "e2e:open": "grafana-e2e open",
    "e2e:update": "grafana-e2e run --update-screenshots",
    "dev": "grafana-toolkit plugin:dev",
    "watch": "grafana-toolkit plugin:dev --watch"
  },
  "repository": "github:grafana/github-datasource",
  "author": "Grafana Labs <team@grafana.com> (https://grafana.com)",
  "license": "Apache-2.0",
  "dependencies": {
    "@emotion/css": "^11.10.0",
    "@grafana/data": "8.4.7",
    "@grafana/runtime": "8.4.7",
    "@grafana/ui": "8.4.7"
  },
  "devDependencies": {
    "@grafana/e2e": "8.4.7",
    "@grafana/e2e-selectors": "8.4.7",
    "@grafana/toolkit": "8.4.7",
    "@types/grafana": "github:CorpGlory/types-grafana.git",
    "@types/lodash": "latest",
    "cypress": "7.7.0"
  }
}
