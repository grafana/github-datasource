{
  "name": "grafana-github-datasource",
  "version": "1.0.6",
  "description": "loads data from github issues/Pr's to Grafana",
  "scripts": {
    "build": "rm -rf node_modules/@grafana/data/node_modules; grafana-toolkit plugin:build",
    "dev": "rm -rf node_modules/@grafana/data/node_modules; grafana-toolkit plugin:dev",
    "test": "rm -rf node_modules/@grafana/data/node_modules; grafana-toolkit plugin:test",
    "test:e2e": "grafana-e2e run",
    "test:e2e:update": "grafana-e2e run --update-screenshots",
    "watch": "rm -rf node_modules/@grafana/data/node_modules; grafana-toolkit plugin:dev --watch"
  },
  "repository": "github:grafana/github-datasource",
  "author": "Grafana Labs <team@grafana.com> (https://grafana.com)",
  "license": "Apache-2.0",
  "devDependencies": {
    "@grafana/data": "7.1.5",
    "@grafana/e2e": "canary",
    "@grafana/runtime": "7.1.5",
    "@grafana/toolkit": "^7.2.0",
    "@grafana/ui": "7.1.5",
    "@types/grafana": "github:CorpGlory/types-grafana.git",
    "@types/lodash": "latest"
  }
}
